<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <!-- GIS plugin Informations -->
    <name>gis</name>
    <class>fr.paris.lutece.plugins.gis.service.GisPlugin</class>
    <version>2.2.2</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>gis.plugin.description</description>
    <provider>gis.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/gis/gis.png</icon-url>
    <copyright>Copyright 2001-2011 Mairie de Paris</copyright>
    <core-version-dependency>
		<min-core-version>2.4.0</min-core-version>
		<max-core-version/>	
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>   

    <!--  Specific plugin CSS stylesheet   --> 
    <css-stylesheets>
          <css-stylesheet>gis/gis.css</css-stylesheet> 
    </css-stylesheets>
    
    <admin-features>
        <admin-feature>
            <feature-id>GIS_MANAGEMENT</feature-id>
            <feature-title>gis.adminFeature.gis_management.name</feature-title>
            <feature-description>gis.adminFeature.gis_management.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/gis/ManageGis.jsp</feature-url>
            <feature-group>APPLICATIONS</feature-group>
            <feature-icon-url>images/admin/skin/plugins/gis/gis_feature.png</feature-icon-url>
        </admin-feature>
    </admin-features>
    
    <!-- Application XPage  -->
    <applications>
       <application>
           <application-id>gis</application-id>
           <application-class>fr.paris.lutece.plugins.gis.web.xpage.GisXPageApplication</application-class>
       </application>
    </applications>    
</plug-in>